<Window x:Class="TimberYardSoft.AddYardWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="AddYardWindow" Height="200" Width="200">
    <Canvas Background="GreenYellow">
        <TextBlock Canvas.Left="41" TextWrapping="Wrap" Text="Enter new yard's name:" Canvas.Top="30"/>
        <TextBox Name="YardName" Height="23" Canvas.Left="41" TextWrapping="Wrap"  Canvas.Top="46" Width="122" RenderTransformOrigin="0.55,-0.87"/>
        <Button Name="OKButton" Click="OnOKButton_Click" Content="Create yard" Canvas.Left="62" Canvas.Top="93" Width="75"/>

    </Canvas>
</Window>
